{
  "extends": [
    "airbnb"
  ],
  "globals": {
    "document": true,
    "window": true
  },
  "rules": {
    "comma-dangle": "off",
    "no-plusplus": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "react/prop-types": "off",
    "react/jsx-filename-extension": "off",
    "max-len": [
      "error",
      {
        "code": 100
      }
    ],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "react/jsx-indent": [
      "error",
      2
    ],
    "react/jsx-indent-props": [
      "error",
      2
    ],
    "object-curly-newline": "off",
    "import/prefer-default-export": "off",
    "global-require": "off",
    "no-shadow": "off"
  }
}